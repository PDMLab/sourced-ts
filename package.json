{
  "name": "sourced-ts",
  "version": "0.0.9",
  "description": "Tiny TypeScript framework for building models with the event sourcing  pattern (events and snapshots).",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src __tests__",
    "lint:fix": "eslint --fix src __tests__",
    "test": "jest --config jest.config.js --coverage --verbose",
    "test:watch": "jest --config jest.config.js --watch --coverage --verbose",
    "precommit": "npm run lint:fix && npm run test",
    "prepublish": "npm run build",
    "prepare": "husky install",
    "release": "yarn build && standard-version"
  },
  "keywords": [
    "event-sourcing",
    "eventsourcing",
    "typescript"
  ],
  "author": {
    "name": "Alexander Zeitler",
    "email": "alexander.zeitler@pdmlab.com"
  },
  "contributors": [
    "mattwalters5@gmail.com",
    "pat@patscott.io",
    "alexander.zeitler@pdmlab.com"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pdmlab/sourced-ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/lodash.clonedeep": "^4.5.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "14",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-watch": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "27.0.4",
    "prettier": "^2.3.1",
    "regenerator-runtime": "0.13.5",
    "sinon": "^11.1.1",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2"
  }
}
